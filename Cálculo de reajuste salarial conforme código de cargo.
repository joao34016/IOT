#include <stdio.h>

int main()
{
    float salarioA, salarioF;
    int cargo;
    printf("Qual o seu sal치rio? ");
    scanf("%f", &salarioA);
    printf("Qual o seu cargo: \n 1- Auxiliar de escrit칩rio\n 2- Secret치rio\n 3-  Coziheiro(a)\n 4- Enregador(a)");
    scanf("%i", &cargo);
    switch (cargo){
        case 1:
            salarioF = salarioA + salarioA*7/100;
            printf("salario = %.2f", salarioF);
            break;
        case 2:
            salarioF = salarioA + salarioA*9/100;
            printf("salario = %.2f", salarioF);
            break;
        case 3:
            salarioF = salarioA + salarioA*5/100;
            printf("salario = %.2f", salarioF);
            break;
        case 4:
            salarioF = salarioA + salarioA*12/100;
            printf("salario = %.2f", salarioF);
            break;
        default:
            printf("Inv치lido.");
            break;
    }

    return 0;
}
